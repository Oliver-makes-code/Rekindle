pub class SomeClass(pub owo: int) {
    pub fun get_owo(this) int {
        return owo
    }

    defer Number -> owo

    impl Default {
        fun default() This {
            return SomeClass(0)
        }
    }
}

fun get_default(T: typeof Default) T {
    return T.default()
}

fun square(n: Number) Number {
    return n * n
}

object SomeObject {
    a: int
    b: string
}

object AnotherObject {
    a: int
}

fun get_a(obj: AnotherObject) int {
    return obj.a
}

fun main() {
    let some_class = get_default(SomeClass)
    let s = square(some_class)
    let some_obj = SomeObject {
        a = 15
        b = "owo"
    }
    let a = get_a(some_obj)

    print(s)
    print(a)
}

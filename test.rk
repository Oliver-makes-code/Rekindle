pub class SomeClass(pub owo: int) {
    pub fun get_owo(this) int {
        return owo
    }

    impl Default {
        fun default() This {
            return SomeClass(0)
        }
    }
}

fun get_default(T: typeof Default) T {
    return T.default()
}

